<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="TfsUrl" value="https://tfs03.codeplex.com"/>
  </appSettings>
  <connectionStrings/>
  <system.net>
    <defaultProxy>
      <proxy bypassonlocal="True" proxyaddress="http://itgproxy.redmond.corp.microsoft.com" usesystemdefault="False" />
    </defaultProxy>
  </system.net>
  <system.web>
    <compilation debug="true" />
    <authentication mode="None" />
    <httpHandlers>
      <add verb="*" path="*" type="SvnBridge.Web.SvnBridgeHttpHandler, SvnFacade.Library" />
    </httpHandlers>
  </system.web>
</configuration>
